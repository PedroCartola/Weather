<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myproject.weatherapi.controller.ApiController">
   <children>
      <Button fx:id="btnBuscar" layoutX="45.0" layoutY="216.0" mnemonicParsing="false" onAction="#enviaInformcao" prefHeight="30.0" prefWidth="230.0" text="Buscar" />
      <TextField fx:id="txtCidade" layoutX="45.0" layoutY="145.0" prefHeight="30.0" prefWidth="230.0" promptText="Cidade" />
      <ButtonBar layoutX="5.0" prefHeight="40.0" prefWidth="310.0">
        <buttons>
            <Label fx:id="lblNome" />
            <Button fx:id="btnVoltar" mnemonicParsing="false" onAction="#voltarTela" style="-fx-background-color: #363636;" text="Sair" textFill="WHITE" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
